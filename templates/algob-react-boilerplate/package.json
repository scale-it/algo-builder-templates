{
  "name": "algob-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^4.2.0",
    "react-app-rewire-yaml": "^1.1.0",
    "react-app-rewired": "^2.1.8",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "husky": "4.3.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@algorand-builder/algob": "^0.4.0",
    "@material-ui/core": "^4.11.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "algosdk": "^1.8.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint '*/*.{js,ts}'",
    "lint:fix": "eslint --fix --ext .js,.ts scripts src",
    "format": "prettier --write src/**/*.{js,jsx,css,scss}"
  },
  "lint-staged": {
    "**/{src,test,scripts}/**/*.{ts,js}": [
      "eslint --fix --ext .js,.ts scripts src",
      "prettier --write src/**/*.{js,jsx,css,scss}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
